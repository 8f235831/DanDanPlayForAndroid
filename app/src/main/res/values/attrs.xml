<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ExpandableTextView">
        <attr name="expandableLines" format="integer" />
    </declare-styleable>

    <declare-styleable name="SemicircleView">
        <attr name="curvature" format="float" />
        <attr name="distance_top" format="dimension" />
        <attr name="mask_out_color" format="color|reference"/>
        <attr name="mask_in_color" format="color|reference"/>
    </declare-styleable>

    <declare-styleable name="ShadowLayout">
        <attr name="shadowRadius" format="dimension" />
        <attr name="shadowElevation" format="dimension" />
        <attr name="shadowAlpha" format="float" />

        <attr name="bottomDividerHeight" format="dimension" />
        <attr name="bottomDividerColor" format="color|reference" />
        <attr name="bottomDividerInsetLeft" format="dimension" />
        <attr name="bottomDividerInsetRight" format="dimension" />
        <attr name="topDividerHeight" format="dimension" />
        <attr name="topDividerColor" format="color|reference" />
        <attr name="topDividerInsetLeft" format="dimension" />
        <attr name="topDividerInsetRight" format="dimension" />
        <attr name="leftDividerWidth" format="dimension" />
        <attr name="leftDividerColor" format="color|reference" />
        <attr name="leftDividerInsetTop" format="dimension" />
        <attr name="leftDividerInsetBottom" format="dimension" />
        <attr name="rightDividerWidth" format="dimension" />
        <attr name="rightDividerColor" format="color|reference" />
        <attr name="rightDividerInsetTop" format="dimension" />
        <attr name="rightDividerInsetBottom" format="dimension" />
        <attr name="borderColor" format="color" />
        <attr name="borderWidth" format="dimension" />
        <attr name="outerNormalColor" format="color|reference" />
        <attr name="hideRadiusSide" format="enum">
            <enum name="none" value="0" />
            <enum name="top" value="1" />
            <enum name="right" value="2" />
            <enum name="bottom" value="3" />
            <enum name="left" value="4" />
        </attr>
        <attr name="showBorderOnlyBeforeL" format="boolean" />
        <attr name="outlineInsetTop" format="dimension" />
        <attr name="outlineInsetLeft" format="dimension" />
        <attr name="outlineInsetRight" format="dimension" />
        <attr name="outlineInsetBottom" format="dimension" />
        <attr name="outlineExcludePadding" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ScanWindowView">
        <attr name="show_frame" format="boolean"/>
        <attr name="line_color" format="color"/>
        <attr name="rect_color" format="color"/>
        <attr name="frame_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="SlantedTextView">
        <attr name="slantedTextSize" format="dimension"/>
        <attr name="slantedBackgroundColor" format="color"/>
        <attr name="slantedText" format="string"/>
        <attr name="slantedTextColor" format="color"/>
        <attr name="slantedLength" format="dimension"/>
        <attr name="slantedMode" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="left_bottom" value="2" />
            <enum name="right_bottom" value="3" />
            <enum name="left_triangle" value="4" />
            <enum name="right_triangle" value="5" />
            <enum name="left_bottom_triangle" value="6" />
            <enum name="right_bottom_triangle" value="7" />
        </attr>
    </declare-styleable>



    <declare-styleable name="TextPathAnimView">
        <attr name="duration" format="integer" />
        <attr name="text" format="string" />
        <attr name="text_bg_color" format="color" />
        <attr name="text_fg_color" format="color" />
        <attr name="text_size_scale" format="float" />
        <attr name="text_stoke_width" format="float" />
        <attr name="text_interval" format="dimension" />
        <attr name="loop" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AnimatedSvgView">
        <attr name="animatedSvgImageSizeX" format="integer"/>
        <attr name="animatedSvgImageSizeY" format="integer"/>
        <attr name="animatedSvgTraceTime" format="string"/>
        <attr name="animatedSvgTraceTimePerGlyph" format="string"/>
        <attr name="animatedSvgFillStart" format="string"/>
        <attr name="animatedSvgFillTime" format="string"/>
        <attr name="animatedSvgGlyphStrings" format="reference"/>
        <attr name="animatedSvgTraceColors" format="reference"/>
        <attr name="animatedSvgTraceResidueColors" format="reference"/>
        <attr name="animatedSvgTraceMarkerLength" format="integer"/>
        <attr name="animatedSvgFillColors" format="reference"/>
    </declare-styleable>

    <declare-styleable name="CircleProgressView">
        <!--背景色-->
        <attr name="progressBackgroundColor" format="reference" />
        <!--进度条颜色-->
        <attr name="progressColor" format="reference" />
        <!--进度-->
        <attr name="progress" format="integer" />
        <!--进度条宽度-->
        <attr name="progressWidth" format="dimension" />
        <!--字体大小-->
        <attr name="textSize" format="dimension" />
        <!--字体颜色-->
        <attr name="textColor" format="reference" />
    </declare-styleable>

    <declare-styleable name="EasySwipeMenuLayout">
        <attr format="reference" name="contentView"/>
        <attr format="reference" name="topMenuView"/>
        <attr format="reference" name="bottomMenuView"/>
        <attr format="reference" name="leftMenuView"/>
        <attr format="reference" name="rightMenuView"/>
    </declare-styleable>

    <declare-styleable name="ExpandableLayout">
        <attr name="el_duration" format="integer" />
        <attr name="el_expanded" format="boolean" />
        <attr name="el_parallax" format="float" />
        <attr name="android:orientation"/>
    </declare-styleable>
</resources>